{
  "name": "endowcast-server",
  "version": "1.0.0",
  "description": "EndowCast backend API server",
  "main": "src/server.js",
  "scripts": {
    "start": "npx prisma db push --skip-generate && npx prisma generate && node src/server.js",
    "start:prod": "npx prisma migrate deploy && node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "seed": "npx prisma db seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "endowment",
    "modeling",
    "finance",
    "saas"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "nodemailer": "^6.9.4",
    "stripe": "^11.17.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
